{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-clean-code"
  ],
  "jsRules": {},
  "rules": {
    "newspaper-order": true,
    "no-var-keyword": true,
    "no-duplicate-variable": true,
    "max-line-length": [
      true,
      160
    ],
    "no-namespace": false
  },
  "rulesDirectory": []
}